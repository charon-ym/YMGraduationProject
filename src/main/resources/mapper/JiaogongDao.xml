<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.JiaogongDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.JiaogongEntity" id="jiaogongMap">
        <result property="jiaogongzhanghao" column="jiaogongzhanghao"/>
        <result property="mima" column="mima"/>
        <result property="jiaogongxingming" column="jiaogongxingming"/>
        <result property="xingbie" column="xingbie"/>
        <result property="touxiang" column="touxiang"/>
        <result property="lianxidianhua" column="lianxidianhua"/>
        <result property="shenfen" column="shenfen"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.JiaogongVO" >
		SELECT * FROM jiaogong  jiaogong         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.JiaogongVO" >
		SELECT  jiaogong.* FROM jiaogong  jiaogong 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.JiaogongView" >

		SELECT  jiaogong.* FROM jiaogong  jiaogong 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.JiaogongView" >
		SELECT * FROM jiaogong  jiaogong <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
